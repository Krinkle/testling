{
    "name": "testling",
    "description": "write tests for browser code",
    "version": "1.7.3",
    "repository": {
        "type": "git",
        "url": "git://github.com/substack/testling.git"
    },
    "browserify": "browser.js",
    "bin": {
        "testling": "bin/cmd.js"
    },
    "keywords": [
        "test",
        "browser",
        "headless"
    ],
    "scripts": {
        "prebuild": "rimraf bundle",
        "build": "mkdir -p bundle && browserify browser/prelude.js -o bundle/prelude.js",
        "prepublishOnly": "safe-publish-latest && npm run build",
        "prepublish": "not-in-publish || npm run prepublishOnly"
    },
    "dependencies": {
        "bouncy": "~3.2.2",
        "browser-launcher": "~1.0.1",
        "browserify": "3.x.x",
        "concat-stream": "~1.0.1",
        "ecstatic": "~0.4.13",
        "ent": "~0.0.7",
        "glob": "~3.2.11",
        "jsonify": "~0.0.0",
        "object-inspect": "~1.7.0",
        "optimist": "~0.5.2",
        "resolve": "~1.14.2",
        "shallow-copy": "~0.0.1",
        "shell-quote": "~1.7.2",
        "tap-finished": "~0.0.1",
        "win-spawn": "~2.0.0",
        "xhr-write-stream": "~0.1.2"
    },
    "devDependencies": {
        "in-publish": "~2.0.1",
        "rimraf": "~3.0.2",
        "safe-publish-latest": "^2.0.0",
        "tape": "~5.6.1"
    },
    "engines": {
        "node": ">=0.6"
    },
    "license": "MIT/X11",
    "author": "Jordan Harband <ljharb@gmail.com>",
    "funding": {
        "url": "https://github.com/sponsors/ljharb"
    }
}
